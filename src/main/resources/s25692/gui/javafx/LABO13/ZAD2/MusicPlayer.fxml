<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="s25692.gui.javafx.LABO13.ZAD2.Controller">
    <Label text="Odtwarzacz muzyki"/>
    <Label text="Obecnie odtwarzany utwór:"/>
    <Label fx:id="nowPlayingLabel"/>
    <ListView fx:id="categoriesListView" prefHeight="200.0" prefWidth="200.0"/>
    <TextField fx:id="searchTextField" promptText="Wyszukaj piosenkę, album lub twórcę"/>
    <Button text="Zatrzymaj" onAction="#stop"/>
    <Button text="Mieszaj" onAction="#shuffle"/>
    <Button text="Odtwarzaj w pętli" onAction="#playInLoop"/>
</VBox>
